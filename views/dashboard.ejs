<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kamingo Kirana</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>

    <div class="container">
        <h1>Customers List</h1>
        <hr>
        <% lists.forEach(element => { %>
            <form action="/<%= element.number %>" method="post" target="_blank">
                <input type="text" name="user" class="d-none" value="<%= user %>">
                <div class="d-flex justify-content-between align-items-center border p-1 m-2 rounded">

                    <input type="text" class="form-control" name="customername" value="<%= element.name %>">
                    <button  class="btn btn-dark btn-sm mx-2" type="submit">View</button>
                </div>
            </form>
        <% }) %>
    </div>

</body>
</html>